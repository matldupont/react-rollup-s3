{
  "name": "react-rollup-s3",
  "version": "1.0.0",
  "description": "Tooling for React using Rollup and deploying to S3",
  "main": "dist/index.js",
  "style": "dist/styles.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "build:watch" : "rollup -c -w",
    "flow": "flow",
    "devserver": "live-server --open=dist --entry-file=index.html",
    "start": "npm-run-all --parallel devserver build:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matldupont/react-rollup-s3.git"
  },
  "keywords": [
    "react",
    "rollup",
    "s3",
    "babel",
    "eslint"
  ],
  "author": "Mat Dupont",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matldupont/react-rollup-s3/issues"
  },
  "homepage": "https://github.com/matldupont/react-rollup-s3#readme",
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "browser-sync": "2.24.4",
    "npm-run-all": "4.1.3",
    "rollup": "0.59.4",
    "rollup-plugin-babel": "3.0.4",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-globals": "1.2.1",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-uglify": "4.0.0",
    "rollup-watch": "4.3.1"
  }
}
